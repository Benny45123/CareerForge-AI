{
    "builds": [
      { "src": "frontend/package.json", "use": "@vercel/static-build" },
      { "src": "backend/app.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/app.js" },
      {"src":"/assets/(.*)","dest":"frontend/assets/$1"},
      { "src": "/(.*)", "dest": "frontend/index.html" }
    ]
  }
  